{
    "name": "amp-client",
    "version": "1.0.11",
    "description": "Amp client demo",
    "main": "index.ts",
    "scripts": {
        "test": "node ./node_modules/jest/bin/jest.js",
        "tslint": "node ./node_modules/tslint/bin/tslint -p ./tsconfig.json -e '**/dist/**'",
        "build": "./node_modules/.bin/webpack --color --config configs/webpack.prod.js --mode production",
        "dockerbuild": "node ./scripts/dockerBuild.js --docker-build",
        "dockerpush": "node ./scripts/dockerBuild.js --docker-push",
        "preversion": "npm test && npm run build",
        "version": "npm run dockerbuild",
        "postversion": "npm run dockerpush",
        "postinstall": "node ./scripts/setupDevEnvironment.js",
        "start": "./node_modules/.bin/webpack-dev-server --config ./configs/webpack.dev.js --mode development"
    },
    "author": "sseiber",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git@github.com:sseiber/AMP-client.git"
    },
    "dependencies": {
        "babel-polyfill": "^6.26.0",
        "eventemitter2": "^4.1.0",
        "fs-extra": "^8.1.0",
        "history": "^4.10.1",
        "lodash.get": "^4.4.2",
        "lodash.pick": "^4.4.0",
        "mobx": "^5.15.1",
        "mobx-react": "^6.1.4",
        "moment": "^2.25.3",
        "query-string": "^6.9.0",
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "react-router-dom": "^5.1.2",
        "semantic-ui-react": "^0.88.2",
        "whatwg-fetch": "^3.0.0"
    },
    "devDependencies": {
        "@hot-loader/react-dom": "^16.11.0",
        "@types/enzyme": "^3.10.4",
        "@types/history": "^4.7.3",
        "@types/jest": "^24.0.25",
        "@types/node": "^13.1.4",
        "@types/react": "^16.9.17",
        "@types/react-test-renderer": "^16.9.1",
        "@types/webpack-env": "^1.15.0",
        "awesome-typescript-loader": "^5.2.1",
        "babel-core": "^6.26.3",
        "babel-preset-env": "^1.7.0",
        "commander": "^4.0.1",
        "copy-webpack-plugin": "^5.1.1",
        "css-loader": "^3.4.1",
        "enzyme": "^3.11.0",
        "extract-text-webpack-plugin": "^4.0.0-beta.0",
        "file-loader": "^5.0.2",
        "html-webpack-plugin": "^3.2.0",
        "jest": "^24.9.0",
        "node-sass": "^4.13.1",
        "react-hot-loader": "^4.12.18",
        "react-test-renderer": "^16.12.0",
        "sass-loader": "^8.0.0",
        "style-loader": "^1.1.2",
        "ts-jest": "^26.0.0",
        "tslint": "^5.20.1",
        "tslint-react": "^4.1.0",
        "typescript": "^3.7.5",
        "url-loader": "^3.0.0",
        "webpack": "^4.41.5",
        "webpack-cli": "^3.3.10",
        "webpack-dev-server": "^3.10.1"
    },
    "jest": {
        "testRegex": "(/__tests__/.*|\\.spec)\\.(ts|tsx)$",
        "moduleFileExtensions": [
            "js",
            "jsx",
            "json",
            "ts",
            "tsx"
        ],
        "transform": {
            ".tsx?": "ts-jest"
        },
        "moduleNameMapper": {
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "./__mocks__/fileMock.js"
        }
    }
}
